      <DIAGRAM FORMAT="1.00" ID="6" NAME="setupReflexions(): array of array[0..1] of string" CREATED="2016.10.09 12:00:00" MODIFIED="2022.01.11 12:00:00" >
        <LAYOUT FORMAT="1.00" COLUMNS="2" ROWS="13">
